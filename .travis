"language": "java",
  "jdk": "oraclejdk8",
  "addons": {
    "postgresql": "9.3"
  },
  "sonarqube": {
    "branches": [
      "develop",
      "master"
    ]
  },
  "script": [
    "sonar-scanner -X -Dsonar.host.url=https://sonarqube.com -Dsonar.login=$SONAR_TOKEN",
    "mvn -e sonar:sonar -Dsonar.login=$SONAR_TOKEN -Dsonar.host.url=https:/sonarqube.com",
    "mvn -e -B package"
  ],
  "global_env": "SONAR_TOKEN=[secure]",
  "group": "stable",
  "dist": "precise",
  "os": "linux"

